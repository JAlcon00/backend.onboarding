    "nodemon": "^3.1.10",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
