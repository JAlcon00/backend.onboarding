{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=2048 --expose-gc\" nodemon src/server.ts",
    "dev:debug": "cross-env NODE_OPTIONS=\"--max-old-space-size=2048 --expose-gc --inspect\" nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "test": "jest",
    "test:usuario": "jest test/integration/usuario.test.ts --verbose --detectOpenHandles --forceExit",
    "test:usuario-simple": "jest test/integration/usuario-simple.test.ts --verbose --detectOpenHandles --forceExit",
    "test:usuario-working": "jest test/integration/usuario-working.test.ts --verbose --detectOpenHandles --forceExit",
    "test:fast": "vitest",
    "test:cliente": "jest test/modules/cliente/ --verbose",
    "test:cliente:model": "jest test/modules/cliente/cliente.model.test.ts --verbose",
    "test:cliente:service": "jest test/modules/cliente/cliente.service.test.ts --verbose",
    "test:cliente:simple": "jest test/modules/cliente/cliente.simple.test.ts --verbose",
    "test:security": "jest test/integration/usuarios*.test.ts --verbose --detectOpenHandles --forceExit",
    "test:security:basic": "jest test/integration/usuarios.test.ts --verbose --detectOpenHandles --forceExit",
    "test:security:advanced": "jest test/integration/usuarios-security.test.ts --verbose --detectOpenHandles --forceExit",
    "test:security:full": "jest test/integration/usuarios-advanced-security.test.ts --verbose --detectOpenHandles --forceExit",
    "test:coverage": "jest --coverage",
    "test:security:coverage": "jest test/integration/usuarios*.test.ts --coverage --coverageDirectory=coverage/security"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "busboy": "^1.6.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.1",
    "file-type": "^21.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "node-cron": "^4.2.1",
    "pino": "^9.7.0",
    "redis": "^5.6.0",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.2",
    "@types/busboy": "^1.5.4",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
